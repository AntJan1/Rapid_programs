# !/bin/bash
mkdir results
touch vector.txt
touch vep.txt
touch eigenvalues.txt
rm vector.txt vep.txt eigenvalues.txt
touch vector.txt
touch vep.txt
touch eigenvalues.txt
gcc final.c -o macierz -lm
for ((i=5;i<=${1};i+=2))
do
echo robie $i
./macierz $i
sort eigenvalues.txt | uniq -c > results/$i.txt
rm eigenvalues.txt
done
exit
